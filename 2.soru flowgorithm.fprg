                <assign variable="toplam2" expression="toplam2 + x"/>
            </for>
            <assign variable="y" expression="toplam2 * toplam2"/>
            <comment text="y : say&#305;lar&#305;n toplam&#305;n&#305;n karesinin sonucu"/>
            <output expression="y - toplam" newline="True"/>
        </body>
