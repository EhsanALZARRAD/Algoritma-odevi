    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="toplam, toplam2, y, x, r, e" type="Integer" array="False" size=""/>
            <comment text="r : her bir say&#305;n&#305;n karesidir"/>
            <assign variable="toplam2" expression="0"/>
